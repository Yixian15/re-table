{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./",
      "paths": {
        "@src/*": ["src/*"],
        "@Table/*": ["src/Table/*"]
      }
  },
  "includes": [
    "./src/",
  ]
}